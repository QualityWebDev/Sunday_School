{
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "/good-news/gospel.html",
        "use": "@vercel/static"
      },
      {
        "src": "styles.css",
        "use": "@vercel/static"
      },
      {
        "src": "/good-news/cover.jpg",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/good-news/(.*)",
        "dest": "/good-news/gospel.html"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  